{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "lib": [
      "dom",
      "es2015",
      "es2017"
    ],
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": []
  },
  "compileOnSave": false
}
