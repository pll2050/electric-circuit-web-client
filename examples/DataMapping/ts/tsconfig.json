{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es5",
    "lib": [
      "dom",
      "esnext",
      "es2015",
      "es2017"
    ],
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "skipLibCheck": true
  },
  "compileOnSave": false
}
